{
    "compilerOptions": {
        "target": "ES2023",
        "module": "CommonJS",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "checkJs": true,
		"removeComments": false,
        "resolveJsonModule": true,
        "paths": {
            "types": ["./types.ts"]
        }
    },
    "exclude": [
        "node_modules",
        "PS Docs"
    ],
}
